alias valgrind='valgrind --track-origins=yes --leak-check=full -v'
alias update_system='sudo apt-get update && sudo apt-get upgrade && sudo apt-get autoremove'

# Git aliases
alias gs='git status'
alias ga='git add '
alias gaa='git add --all'
alias gc='git commit'
alias gcm='git commit -m '
alias gp='git push'
